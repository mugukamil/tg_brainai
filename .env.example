# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ASSISTANT_ID=your_openai_assistant_id_here

# GoAPI Configuration (for Midjourney image generation)
GOAPI_API_KEY=your_goapi_api_key_here

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Replicate Configuration (for video generation)
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Optional: Bot Configuration
# BOT_USERNAME=your_bot_username (without @)
# BOT_WEBHOOK_URL=https://your-domain.com/webhook (if using webhooks instead of polling)

# Optional: Rate Limiting
# DEFAULT_TEXT_REQUESTS=100
# DEFAULT_IMAGE_REQUESTS=10
# DEFAULT_VIDEO_REQUESTS=5
# PREMIUM_TEXT_REQUESTS=1000
# PREMIUM_IMAGE_REQUESTS=100
# PREMIUM_VIDEO_REQUESTS=50

# Optional: Feature Flags
# ENABLE_PAYMENTS=true
# ENABLE_VIDEO_GENERATION=true
# ENABLE_VOICE_PROCESSING=true
# ENABLE_IMAGE_ANALYSIS=true

# Webhook Configuration (for local development and production)
# USE_WEBHOOK=true
# WEBHOOK_URL=https://your-domain.com/webhook
# WEBHOOK_SECRET_TOKEN=your_secret_token_here
# PORT=3000

# Ngrok Configuration (for local development only)
# NGROK_AUTH_TOKEN=your_ngrok_auth_token_here
# NGROK_REGION=us

# Optional: Logging
# LOG_LEVEL=info
# ENABLE_DEBUG_LOGGING=false
